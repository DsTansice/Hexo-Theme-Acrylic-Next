<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>
    <%= page.title || config.title %>
    <% if (config.subtitle){ %>
        - <% config.subtitle %>
    <% } %>
</title>
<noscript>开启Javascript是必要的</noscript>
